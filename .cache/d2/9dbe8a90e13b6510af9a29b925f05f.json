n  /* border: 1px solid red; */\r\n}\r\n.MainMenuModal {\r\n  width: 50%;\r\n  height: 450px;\r\n  position: -webkit-sticky; /* Safari */\r\n  position: absolute;\r\n  left: 25%;\r\n  background-color: rgb(245, 241, 241);\r\n  border: 1px solid black;\r\n  z-index: 1;\r\n  background-image: linear-gradient(to Bottom right, white, #5b5c5e, white);\r\n}\r\n.ITServices:hover {\r\n  cursor: pointer;\r\n}\r\n\r\n.closeSubMenu {\r\n  cursor: pointer;\r\n}\r\n/* .grid-container {\r\n  display: grid;\r\n  grid-template-columns: auto auto auto auto;\r\n  background-color: #2196f3;\r\n  padding: 10px;\r\n  width: 90vw;\r\n  margin: 1px solid red;\r\n}\r\n\r\n\r\n.grid-item {\r\n  background-color: rgba(255, 255, 255, 0.8);\r\n  border: 1px solid rgba(0, 0, 0, 0.8);\r\n  padding: 20px;\r\n  font-size: 30px;\r\n  text-align: center;\r\n} */\r\n#landing_page_content {\r\n  margin: auto;\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n}\r\n.one {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  /* padding-left: 2%; */\r\n  display: flex;\r\n  flex-direction: row;\r\n  border: 1px solid red;\r\n}\r\n.oneDotOne,\r\n.oneDotTwo,\r\n.oneDotThree,\r\n.oneDotFour {\r\n  /* float: left; */\r\n  border: 1px solid #1c1a38;\r\n  box-sizing: border-box;\r\n  width: 25%;\r\n  border-radius: 5px;\r\n  box-shadow: -5px 2px 0 0 #1c1a38;\r\n}\r\n.flexContainerFooter {\r\n  box-sizing: border-box;\r\n  width: 100%;\r\n  background-color: #879187;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n.flexOne {\r\n  /* border: 1px yellow solid;\r\n  background-color: blue; */\r\n  display: flex;\r\n  box-sizing: border-box;\r\n  width: 70%;\r\n\r\n  flex-wrap: wrap;\r\n  justify-content: space-around;\r\n}\r\n.flexTwo {\r\n  display: flex;\r\n  /* box-sizing: border-box; */\r\n  width: 29%;\r\n  justify-content: space-around;\r\n  /* border: 1px solid green; */\r\n  padding-top: 5px;\r\n}\r\n.flexThree {\r\n  width: 100%;\r\n  text-align: center;\r\n  box-sizing: border-box;\r\n  padding-bottom: 5px;\r\n  /* background-color: #879187; */\r\n}\r\n\r\n.contactUsMain {\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 100%;\r\n}\r\n\r\n.contactUsBanner {\r\n  background-color: #2e2761;\r\n}\r\n\r\n.contactUsBannerImage {\r\n  width: 50%;\r\n  height: 400px;\r\n  display: block;\r\n  margin-left: 25%;\r\n}\r\n.contactUsAllMethods {\r\n  display: flex;\r\n}\r\n.formDiv {\r\n  text-align: center;\r\n  width: 25%;\r\n}\r\n.callImage {\r\n  width: 25%;\r\n}\r\n.letters {\r\n  width: 25%;\r\n}\r\n.mailbox {\r\n  width: 25%;\r\n}\r\n\r\n@media (max-width: 500px) {\r\n  .one {\r\n    flex-wrap: wrap;\r\n  }\r\n  .oneDotOne,\r\n  .oneDotTwo,\r\n  .oneDotThree,\r\n  .oneDotFour {\r\n    width: 50%;\r\n  }\r\n}\r\n\r\n@media (max-width: 800px) {\r\n  .contactUsBannerImage {\r\n    width: 100%;\r\n    margin-left: 0px;\r\n  }\r\n\r\n  .contactUsAllMethods {\r\n    flex-wrap: wrap;\r\n  }\r\n  .formDiv,\r\n  .callImage,\r\n  .letters,\r\n  .mailbox {\r\n    width: 50%;\r\n  }\r\n\r\n  .flexContainerFooter {\r\n    flex-direction: column;\r\n  }\r\n  .flexOne {\r\n    display: flex;\r\n    box-sizing: border-box;\r\n    width: 100%;\r\n    flex-wrap: wrap;\r\n    justify-content: \"space-around\";\r\n  }\r\n  .flexTwo {\r\n    display: flex;\r\n    box-sizing: border-box;\r\n    width: 100%;\r\n\r\n    justify-content: space-around;\r\n  }\r\n}\r\n\r\n.flexOne > div {\r\n  box-sizing: border-box;\r\n  /* border: 1px solid green; */\r\n  vertical-align: middle;\r\n  text-align: center;\r\n  line-height: 50px;\r\n}\r\n\r\n.flexTwo > img {\r\n  box-sizing: border-box;\r\n}\r\n"},"lineCount":214}},"error":null,"hash":"01115110c1b34354067170eb40a35d43","cacheData":{"env":{}}}